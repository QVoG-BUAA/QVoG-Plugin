{
  "name": "qvog-plugin",
  "displayName": "QVog-Plugin",
  "description": "this is a plugin for the project QVoG",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qvog-plugin.config",
        "title": "Config"
      },
      {
        "command": "qvog-plugin.convert",
        "title": "Convert"
      },
      {
        "command": "qvog-plugin.query",
        "title": "Query"
      }
    ],
    "submenus": [
      {
        "id": "QVoG-Plugin",
        "label": "QVoG-Plugin"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "QVoG-Plugin",
          "group": "editor",
          "when": "editorFocus"
        }
      ],
      "QVoG-Plugin": [
        {
          "command": "qvog-plugin.config",
          "group": "editor"
        },
        {
          "command": "qvog-plugin.convert",
          "group": "editor"
        },
        {
          "command": "qvog-plugin.query",
          "group": "editor"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}
